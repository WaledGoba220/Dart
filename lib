  int x = 5;
  int y = 10;

  if (x > y) {
    print('$x is greater than $y');
  } else {
    print('$x is less than $y');
  }
}

ğŸ”´print last message Becaues x is less than y 



void main() {
  int x = 10;
  int y = 10;

  if (x > y) {
    print('$x is greater than $y');
  } else if (x == y) {
    print('$x is equal than $y');
  } else {
    print('$x is less than $y');
  }
}

ğŸ”´print message Equal Becaues x is equal than y 
